{
  "manage_answer_scripts": {
      "GET": {
          "status_code": 200,
          "context": {
              "exams": "list"
          }
      }
  },
  "view_attendance_and_scripts": {
      "GET": {
          "status_code": 200,
          "context": {
              "exam": "object",
              "students": "list",
              "answer_scripts": "list"
          },
          "exam_id": 1
      }
  },
  "evaluate_script": {
      "GET": {
          "status_code": 200,
          "context": {
              "answer_script": "object"
          },
          "script_id": 1
      }
  },
  "finalize_management": {
      "POST": {
          "status_code": 302,
          "redirect_url": "/manage/"
      }
  }
}